{
  "name": "jmmacor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean:osx": "find . -name .DS_Store -delete && find . -name '._*' -delete",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "test:ui": "playwright test __tests__ --ui",
    "test:a11y": "playwright test tests/e2e/axe-smoke.spec.ts",
    "test:screenshots": "playwright test tests/e2e/screenshots.spec.ts",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "a11y:lint": "eslint --ext .ts,.tsx .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "import:ingredients": "tsx scripts/import-ingredients.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "seed:recipes": "tsx scripts/seed-recipes.ts"
  },
  "dependencies": {
